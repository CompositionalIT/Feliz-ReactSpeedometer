{
    "private": true,
    "engines": {
        "node": "~18 || ~20",
        "npm": "~9 || ~10"
    },
    "scripts": {
        "prestart": "dotnet fable clean --yes",
        "start": "dotnet fable watch ./src -s -o ./src/output --run npx vite ./src",
        "prebuild": "dotnet fable clean --yes",
        "build": "dotnet fable ./src -s -o ./src/output --run npx vite build ./src",
        "publish-docs": "npm run build && node publish.js"
    },
    "dependencies": {
        "bulma": "^0.9.1",
        "date-fns": "^2.16.1",
        "gh-pages": "^3.1.0",
        "react-d3-speedometer": "^2.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.9.1",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.16",
        "daisyui": "^4.6.0",
        "mocha": "^10.3.0",
        "postcss": "^8.4.31",
        "remotedev": "^0.2.9",
        "sass": "^1.69.5",
        "vite": "^5.0.12"
    }
}
